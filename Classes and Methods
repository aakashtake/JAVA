}
public class Employee{
    
    public void NameofEmployee(Name)
    {
        System.out.println(+Name);
    }
    public void YearofJoining(year)
    {
        System.out.println(+year);
    }
    public void CityofWork(City)
    {
        System.out.println(+City);
    }
}
